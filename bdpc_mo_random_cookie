# 生成随机cookie
def get_cookie():
    seed = "1234567890ABCDEFGHIJKLMNOPQRSTUVWXYZ"
    lis = []
    [lis.append(random.choice(seed)) for _ in range(33)]
    uid = ''.join(lis)
    return cookie_str.format(uid)
